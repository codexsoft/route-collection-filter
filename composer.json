{
    "name": "codexsoft/route-collection-filter",
    "license": "MIT",
    "description": "Simple Symfony RouteCollection filter",
    "authors": [
        {
            "name": "Dmitriy V Kozubskiy",
            "email": "kozubsky@inbox.ru",
            "homepage": "http://codexsoft.ru/",
            "role": "Maintainer"
        }
    ],
    "support": {
